{
  "name": "mapeo-default-settings",
  "version": "2.1.0",
  "description": "Mapeo default config (categories, icons, questions)",
  "dependencies": {},
  "main": "build/presets.json",
  "devDependencies": {
    "mkdirp": "^1.0.4"
    "mapeo-settings-builder": "^3.0.0",
  },
  "scripts": {
    "build": "mkdirp build && mapeo-settings build -l 'es' -o build/${npm_package_name}-v${npm_package_version}.mapeosettings",
    "prepublishOnly": "npm run build && tarball=\"build/${npm_package_name}-v${npm_package_version}.mapeosettings\"; tar -C build -xf $tarball; rm $tarball",
    "test": "mapeo-settings lint"
  },
  "author": "Gregor MacLennan",
  "license": "ISC",
  "files": [
    "dist"
  ]
}
